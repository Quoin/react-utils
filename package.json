{
  "name": "@quoin/react-utils",
  "version": "0.1.2",
  "description": "Utility functions for React",
  "author": {
    "name": "Hữu Ðà Trần",
    "email": "huuda.tran@quoininc.com",
    "url": "https://www.quoininc.com/staff/huu-da-tran/"
  },
  "engines": {
    "node": ">=12.13"
  },
  "publishConfig": {
    "access": "public",
    "tag": "typescript"
  },
  "main": "dist/index.js",
  "scripts": {
    "babel": "babel --out-dir dist --ignore \"src/**/*.test.js\" src",
    "check": "run-s eslint jscpd",
    "clean": "rimraf quoin-react-utils-*.tgz",
    "precoverage": "rimraf reports/nyc_output reports/coverage",
    "coveralls": "cat reports/coverage/lcov.info | coveralls",
    "coverage": "nyc --report-dir=reports/coverage npm test",
    "postcoverage2": "run-s stryker",
    "coverage-report": "nyc report --reporter=text --reporter=text-summary",
    "predocs": "rimraf docs",
    "docs": "typedoc src",
    "eslint": "run-s eslint:code eslint:test",
    "eslint:code": "eslint --fix --config=.eslintrc.code.json \"src/**/*.ts?(x)\"",
    "eslint:test": "eslint --fix --config=.eslintrc.test.json --ignore-pattern=helpers.test.ts \"src/**/*.test.ts?(x)\"",
    "jscpd": "jscpd src",
    "pretsc": "rimraf dist",
    "tsc": "tsc",
    "prepack": "run-s check clean coverage coverage-report tsc",
    "stryker": "stryker run",
    "test": "TS_NODE_FILES=true mocha --exit \"src/**/*.test.ts?(x)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quoin/react-utils.git"
  },
  "publicConfig": {
    "access": "public",
    "tag": "typescript"
  },
  "keywords": [
    "Quoin",
    "React"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quoin/react-utils/issues"
  },
  "homepage": "https://github.com/Quoin/react-utils#readme",
