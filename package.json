{
  "name": "@quoin/react-utils",
  "version": "1.0.0",
  "description": "Utility functions for React",
  "author": "",
  "main": "dist/index.js",
  "scripts": {
    "babel": "babel --out-dir dist --ignore \"src/**/*.test.js\" src",
    "check": "npm run eslint && npm run jscpd",
    "precoverage": "npm run check",
    "coverage": "nyc --report-dir=reports/coverage npm test",
    "coverage-report": "nyc report --reporter=text --reporter=text-summary",
    "eslint": "npm run eslint:node && npm run eslint:client && npm run eslint:client-test",
    "eslint:node": "eslint --fix --config=.eslintrc.node.json src/helpers.test.js",
    "eslint:client": "eslint --fix --config=.eslintrc.client.json --ignore-pattern=\"*.test.js\" --ignore-pattern=\"*.test.jsx\" src",
    "eslint:client-test": "eslint --fix --config=.eslintrc.client.test.json --ignore-pattern=helpers.test.js \"src/**/*.test.js?(x)\"",
    "jscpd": "jscpd src",
    "prepack": "npm run coverage && npm run coverage-report && npm run babel",
    "test": "mocha --require @babel/register --require src/helpers.test.js \"src/**/*.test.js?(x)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quoin/react-utils.git"
  },
  "keywords": [
    "Quoin",
    "React"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quoin/react-utils/issues"
  },
  "homepage": "https://github.com/Quoin/react-utils#readme",
  "devDependencies": {
    "@babel/cli": "~7.7.5",
    "@babel/core": "~7.7.5",
    "@babel/preset-env": "~7.7.6",
    "@babel/preset-react": "~7.7.4",
    "@babel/register": "~7.7.4",
    "@quoin/eslint-config-quoin": "~0.3.3",
    "chai": "~4.2.0",
    "dirty-chai": "~2.0.1",
    "eslint": "~6.7.2",
    "eslint-config-standard": "~14.1.0",
    "eslint-plugin-html": "~6.0.0",
    "eslint-plugin-import": "~2.19.1",
    "eslint-plugin-json": "~2.0.1",
    "eslint-plugin-node": "~10.0.0",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-standard": "~4.0.1",
    "jscpd": "^2.0.16",
    "mocha": "~6.2.2",
    "nyc": "~14.1.1"
  },
  "dependencies": {
    "immutable": "~4.0.0-rc.12",
    "redux": "~4.0.4",
    "redux-logger": "~3.0.6"
  }
}
