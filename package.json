{
  "name": "@quoin/react-utils",
  "version": "1.0.17",
  "description": "Utility functions for React",
  "author": {
    "name": "Hữu Ðà Trần",
    "email": "huuda.tran@quoininc.com",
    "url": "https://www.quoininc.com/staff/huu-da-tran/"
  },
  "engines": {
    "node": ">=12.13"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "scripts": {
    "babel": "babel --out-dir dist --ignore \"src/**/*.test.js\" src",
    "prebuild": "run-s clean",
    "build": "run-s babel",
    "check": "run-s eslint jscpd",
    "check:all": "run-s check coverage",
    "clean": "rimraf dist quoin-react-utils-*.tgz",
    "precoverage": "rimraf reports/nyc_output reports/coverage",
    "coveralls": "cat reports/coverage/lcov.info | coveralls",
    "coverage": "nyc --report-dir=reports/coverage npm test",
    "postcoverage": "run-s stryker",
    "coverage-report": "nyc report --reporter=text --reporter=text-summary",
    "eslint": "eslint --fix --ext=.js,.jsx src",
    "jscpd": "jscpd src",
    "prepack": "run-s check:all coverage-report build",
    "stryker": "stryker run",
    "test": "mocha --exit \"src/**/*.unit.test.js?(x)\"",
    "test:watch": "mocha -w --exit \"src/**/*.unit.test.js?(x)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quoin/react-utils.git"
  },
  "publicConfig": {
    "access": "public"
  },
  "keywords": [
    "Quoin",
    "React"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quoin/react-utils/issues"
  },
  "homepage": "https://github.com/Quoin/react-utils#readme",
  "devDependencies": {
    "@babel/cli": "~7.12.10",
    "@babel/core": "~7.12.10",
    "@babel/preset-env": "~7.12.11",
    "@babel/preset-react": "~7.12.10",
    "@babel/register": "~7.12.10",
    "@stryker-mutator/core": "~3.3.1",
    "@stryker-mutator/javascript-mutator": "~3.3.1",
    "@stryker-mutator/mocha-framework": "~3.3.1",
    "@stryker-mutator/mocha-runner": "~3.3.1",
    "canvas": "~2.6.1",
    "chai": "~4.2.0",
    "chai-immutable": "~2.1.0",
    "classnames": "~2.2.6",
    "coveralls": "~3.1.0",
    "dirty-chai": "~2.0.1",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.5",
    "eslint": "~7.17.0",
    "eslint-config-airbnb": "~18.2.1",
    "eslint-config-standard": "~16.0.2",
    "eslint-plugin-html": "~6.1.1",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-json": "~2.1.2",
    "eslint-plugin-jsx-a11y": "~6.4.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-react": "~7.22.0",
    "eslint-plugin-react-hooks": "~4.2.0",
    "express": "~4.17.1",
    "hal": "~1.2.0",
    "immutable": "~3.8.2",
    "jscpd": "~3.3.23",
    "jsdom": "~16.4.0",
    "mocha": "~8.1.3",
    "npm-run-all": "~4.1.5",
    "nyc": "~15.1.0",
    "prop-types": "~15.7.2",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "react-immutable-proptypes": "~2.2.0",
    "react-redux": "~7.2.2",
    "react-router-dom": "~5.2.0",
    "redux": "~4.0.4",
    "redux-concatenate-reducers": "~1.0.0",
    "redux-logger": "~3.0.6",
    "rimraf": "~3.0.2",
    "sinon": "^9.2.3",
    "sinon-chai": "^3.5.0",
    "source-map-support": "~0.5.19",
    "stryker-cli": "~1.0.0"
  },
  "peerDependencies": {
    "classnames": ">=2.2.6",
    "express": ">=4.17.1",
    "hal": ">=1.2.0",
    "immutable": ">=3.8.2",
    "prop-types": ">=15.7.2",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "react-immutable-proptypes": ">=2.2.0",
    "react-redux": ">=7.2.2",
    "redux": ">=4.0.4",
    "redux-concatenate-reducers": ">=1.0.0",
    "redux-logger": ">=3.0.6",
    "react-router-dom": ">=5.2.0"
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.test.*",
    "*.md"
  ],
  "mocha": {
    "require": [
      "@babel/register",
      "source-map-support/register",
      "./src/setup.test.js"
    ]
  },
  "dependencies": {
    "lodash": "~4.17.20",
    "react-helmet": "~6.1.0",
    "react-loading-skeleton": "~2.1.1",
    "url-template": "~2.0.8"
  }
}
