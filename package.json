{
  "name": "@quoin/react-utils",
  "version": "0.0.7",
  "description": "Utility functions for React",
  "author": {
    "name": "Hữu Ðà Trần",
    "email": "huuda.tran@quoininc.com",
    "url": "https://www.quoininc.com/staff/huu-da-tran/"
  },
  "main": "dist/index.js",
  "scripts": {
    "babel": "babel --out-dir dist --ignore \"src/**/*.test.js\" src",
    "check": "npm run eslint && npm run jscpd",
    "clean": "rimraf dist quoin-react-utils-*.tgz",
    "precoverage": "rimraf reports/nyc_output reports/coverage",
    "coveralls": "cat reports/coverage/lcov.info | coveralls",
    "coverage": "nyc --report-dir=reports/coverage npm test",
    "coverage-report": "nyc report --reporter=text --reporter=text-summary",
    "eslint": "npm run eslint:node && npm run eslint:client && npm run eslint:client-test",
    "eslint:node": "eslint --fix --config=.eslintrc.node.json src/helpers.test.js",
    "eslint:client": "eslint --fix --config=.eslintrc.client.json --ignore-pattern=\"*.test.js\" --ignore-pattern=\"*.test.jsx\" src",
    "eslint:client-test": "eslint --fix --config=.eslintrc.client.test.json --ignore-pattern=helpers.test.js \"src/**/*.test.js?(x)\"",
    "jscpd": "jscpd src",
    "prepack": "npm run check && npm run clean && npm run coverage && npm run coverage-report && npm run babel",
    "test": "mocha \"src/**/*.test.js?(x)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quoin/react-utils.git"
  },
  "keywords": [
    "Quoin",
    "React"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quoin/react-utils/issues"
  },
  "homepage": "https://github.com/Quoin/react-utils#readme",
  "devDependencies": {
    "@babel/cli": "~7.7.5",
    "@babel/core": "~7.7.5",
    "@babel/preset-env": "~7.7.6",
    "@babel/preset-react": "~7.7.4",
    "@babel/register": "~7.7.4",
    "@quoin/eslint-config-quoin": "~0.3.3",
    "canvas": "~2.6.1",
    "chai": "~4.2.0",
    "chai-immutable": "~2.1.0",
    "classnames": "~2.2.6",
    "coveralls": "~3.0.9",
    "dirty-chai": "~2.0.1",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.1",
    "eslint": "~6.8.0",
    "eslint-config-standard": "~14.1.0",
    "eslint-plugin-html": "~6.0.0",
    "eslint-plugin-import": "~2.19.1",
    "eslint-plugin-json": "~2.0.1",
    "eslint-plugin-node": "~11.0.0",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-react": "~7.17.0",
    "eslint-plugin-standard": "~4.0.1",
    "immutable": "~3.8.2",
    "jscpd": "~2.0.16",
    "jsdom": "~15.2.1",
    "mocha": "~7.0.0",
    "nyc": "~15.0.0",
    "prop-types": "~15.7.2",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "react-immutable-proptypes": "~2.1.0",
    "react-redux": "~7.1.3",
    "redux": "~4.0.4",
    "redux-concatenate-reducers": "~1.0.0",
    "redux-logger": "~3.0.6",
    "rimraf": "~3.0.0"
  },
  "peerDependencies": {
    "classnames": ">=2.2.6",
    "immutable": ">=3.8.2",
    "prop-types": ">=15.7.2",
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0",
    "react-immutable-proptypes": ">=2.1.0",
    "react-redux": ">=7.1.3",
    "redux": ">=4.0.4",
    "redux-concatenate-reducers": ">=1.0.0",
    "redux-logger": ">=3.0.6"
  },
  "mocha": {
    "require": [
      "@babel/register",
      "./src/helpers.test.js"
    ]
  }
}
