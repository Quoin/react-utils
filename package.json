    "@types/react-dom": "^16.9.4",
    "@types/react-immutable-proptypes": "^2.1.0",
    "@types/react-redux": "^7.1.5",
    "classnames": ">=2.2.6",
    "immutable": ">=3.8.2",
    "prop-types": ">=15.7.2",
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0",
    "react-immutable-proptypes": ">=2.1.0",
    "react-redux": ">=7.1.3",
    "redux": ">=4.0.4",
    "redux-concatenate-reducers": ">=1.0.0",
    "redux-logger": ">=3.0.6",
    "react-router-dom": ">=5.1.2"
  },
  "files": [
    "dist/**/*",
    "RELEASE-NOTES.md"
  ],
  "types": "./dist/index.d.ts",
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "./src/helpers.test.ts"
    ]
  },
  "dependencies": {
    "url-template": "^2.0.8"
  }
}
