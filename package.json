{
  "name": "@quoin/react-utils",
  "version": "0.0.11",
  "description": "Utility functions for React",
  "author": {
    "name": "Hữu Ðà Trần",
    "email": "huuda.tran@quoininc.com",
    "url": "https://www.quoininc.com/staff/huu-da-tran/"
  },
  "engines": {
    "node": ">=12.13"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "scripts": {
    "babel": "babel --out-dir dist --ignore \"src/**/*.test.js\" src",
    "check": "run-s eslint jscpd",
    "clean": "rimraf dist quoin-react-utils-*.tgz",
    "precoverage": "rimraf reports/nyc_output reports/coverage",
    "coveralls": "cat reports/coverage/lcov.info | coveralls",
    "coverage": "nyc --report-dir=reports/coverage npm test",
    "postcoverage": "run-s stryker",
    "coverage-report": "nyc report --reporter=text --reporter=text-summary",
    "eslint": "run-s eslint:node eslint:client eslint:client-test",
    "eslint:node": "eslint --fix --config=.eslintrc.node.json src/helpers.test.js",
    "eslint:client": "eslint --fix --config=.eslintrc.client.json --ignore-pattern=\"*.test.js\" --ignore-pattern=\"*.test.jsx\" src",
    "eslint:client-test": "eslint --fix --config=.eslintrc.client.test.json --ignore-pattern=helpers.test.js \"src/**/*.test.js?(x)\"",
    "jscpd": "jscpd src",
    "tsc": "tsc",
    "prepack": "run-s clean tsc",
    "prepack2": "run-s check clean coverage coverage-report babel",
    "stryker": "stryker run",
    "test": "TS_NODE_FILES=true mocha \"src/**/*.test.ts?(x)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quoin/react-utils.git"
  },
  "publicConfig": {
    "access": "public"
  },
  "keywords": [
    "Quoin",
    "React"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quoin/react-utils/issues"
  },
  "homepage": "https://github.com/Quoin/react-utils#readme",
  "devDependencies": {
    "@babel/cli": "~7.8.4",
    "@babel/core": "~7.8.4",
    "@babel/preset-env": "~7.8.4",
    "@babel/preset-react": "~7.8.3",
    "@babel/register": "~7.8.3",
    "@quoin/eslint-config-quoin": "~0.3.3",
    "@stryker-mutator/core": "^2.5.0",
    "@stryker-mutator/html-reporter": "^2.5.0",
    "@stryker-mutator/javascript-mutator": "^2.5.0",
    "@stryker-mutator/mocha-framework": "^2.5.0",
    "@stryker-mutator/mocha-runner": "^2.5.0",
    "@types/chai": "^4.2.7",
    "@types/classnames": "^2.2.9",
    "@types/dirty-chai": "^2.0.2",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jsdom": "^12.2.4",
    "@types/mocha": "^5.2.7",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-immutable-proptypes": "^2.1.0",
    "@types/react-redux": "^7.1.5",
    "@types/redux-logger": "^3.0.7",
    "canvas": "~2.6.1",
    "chai": "~4.2.0",
    "chai-immutable": "~2.1.0",
    "classnames": "~2.2.6",
    "coveralls": "~3.0.9",
    "dirty-chai": "~2.0.1",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.1",
    "eslint": "~6.8.0",
    "eslint-config-standard": "~14.1.0",
    "eslint-plugin-html": "~6.0.0",
    "eslint-plugin-import": "~2.20.1",
    "eslint-plugin-json": "~2.0.1",
    "eslint-plugin-node": "~11.0.0",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-react": "~7.18.3",
    "eslint-plugin-standard": "~4.0.1",
    "immutable": "~3.8.2",
    "jscpd": "~2.0.16",
    "jsdom": "~16.1.0",
    "mocha": "~7.0.1",
    "npm-run-all": "^4.1.5",
